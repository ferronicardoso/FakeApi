@model FakeApi.Domain.Dtos.RestMockEditDto

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}

<h2 class="page-header">Editar Mock</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.IdRestMock)
        @Html.Partial("_PartialRestMockForm", Model)
    </div>
}

<script>
    function changeContentType(value) {
        $('#ContentType').val(value);
    }
</script>